{
	ignore_libreadline_leaks
	Memcheck:Leak
	...
	obj:*/libreadline.so.*
}

{
	ignore_regex_library_leak
	Memcheck:Leak
	match-leak-kinds: all
	...
	obj:*/libregex.so.*
}

{
   ls
   Memcheck:Leak
   fun:malloc
   obj:/usr/bin/ls
   ...
}

{
   grep
   Memcheck:Leak
   match-leak-kinds: all
   fun:malloc
   obj:/usr/bin/grep
   ...
}

{
   ls
   Memcheck:Leak
   fun:malloc
   fun:calloc
   obj:/usr/bin/ls
   ...
}
